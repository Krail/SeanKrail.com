extends layout


block main
  main
    each article in content.home.articles
      article(class='home')
        header
          h3=article.heading
        each paragraph in article.paragraphs
          p!=paragraph
        p(class='time')
          small
            - var date = new Date(article.date)
            time(datetime=(date.toISOString()))=date.toDateString()
    article(id='calendar', class='home')
      header
        h3 Check out my schedule
      iframe(src!='https://calendar.google.com/calendar/embed?showTitle=0&amp;showPrint=0&amp;mode=WEEK&amp;height=600&amp;wkst=1&amp;bgcolor=%23FFFFFF&amp;src=skrail%40udel.edu&amp;color=%23875509&amp;src=udel.edu_btb6ifr58b4idr27oae6q4nn1k%40group.calendar.google.com&amp;color=%2329527A&amp;ctz=America%2FNew_York', frameborder='0', scrolling='no')

block script
