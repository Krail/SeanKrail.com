extends layout


block main
  main
    each article in content.home.articles
      article(class='home')
        header
          h3=article.heading
        each paragraph in article.paragraphs
          p!=paragraph
        p(class='time')
          small
            - var date = new Date(article.date)
            time(datetime=(date.toISOString()))=date.toDateString()
    article(id='calendar', class='home')
      header
        h3 Check out my schedule
      iframe(src!='https://calendar.google.com/calendar/embed?showTitle=0&showPrint=0&showCalendars=0&mode=WEEK&height=600&wkst=1&bgcolor=%23ffffff&src=l8aotp7v24dl1mujcvd1j042fs%40group.calendar.google.com&color=%23333333&ctz=America%2FNew_York', frameborder='0', scrolling='no')

block script
