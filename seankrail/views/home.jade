extends layout


block main
  main
    each article in content.home.articles
      article(class='home')
        header
          h3=article.heading
        each paragraph in article.paragraphs
          p!=paragraph
        p(class='time')
          small
            - var date = new Date(article.date)
            time(datetime=(date.toISOString()))=date.toDateString()
    article(class='home')
      header
        h3 Check out my schedule
      iframe(src='https://calendar.google.com/calendar/embed?showTitle=0&amp;showPrint=0&amp;showCalendars=0&amp;mode=WEEK&amp;height=600&amp;wkst=1&amp;bgcolor=%23ffffff&amp;src=l8aotp7v24dl1mujcvd1j042fs%40group.calendar.google.com&amp;color=%23333333&amp;ctz=America%2FNew_York', frameborder='0', scrolling='no')

block script
